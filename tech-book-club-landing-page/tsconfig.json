{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": [
      "astro/client",
      "debug",
      "estree",
      "hast",
      "mdast",
      "ms",
      "nlcst",
      "unist"
    ]
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.astro"],
  "exclude": ["dist"]
}
